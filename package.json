{
  "name": "devdocs",
  "productName": "Devdocs",
  "version": "0.0.0",
  "description": "My polished app",
  "license": "MIT",
  "main": "index.js",
  "repository": "ragingwind/devdocs",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com",
    "url": "ragingwind.me"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "build": {
    "version": "0.29.1",
    "target": "./build"
  },
  "scripts": {
    "start": "electron .",
    "clean": "rm -rf $npm_package_build_target",
    "build": "electron-packager . $npm_package_productName --out=$npm_package_build_target --all --prune --asar --version=$npm_package_build_verison",
    "build darwin": "electron-packager . $npm_package_productName --out=$npm_package_build_target --platform=darwin --arch=x64 --prune --asar --version=$npm_package_build_version"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "electron-debug": "^0.1.1"
  },
  "devDependencies": {
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.29.1"
  }
}
